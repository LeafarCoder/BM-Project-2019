function [g] = BuildForceVector(t)

%Evaluate the splines of forces a COP at time t
Fxy = ppval(fx_sp{k}, time);
Fz = ppval(fy_sp{k}, time);
CoPxy = ppval(CoPx_sp{k}, time);
CoPz = ppval(CoPy_sp{k}, time);
    

%add the gravitational forces
[g] = ForceGravity();

%add the spring-damper-actuator forces - not used
%[g] = ForceSDA(g);

%add contact forces
[g] = ForceContact(g);

end